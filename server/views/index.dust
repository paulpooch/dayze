<header>
    <div id="month_controls">
        <h2 id="month_name">THIS IS MONTH NAME</h2>
    </div>
    <div id="user_controls">
    </div>
</header>
<div id="content">
    <div id="container">
        <div id="calendar"></div>
    </div> 

    <h1>
        <a href="#myModal" role="button" class="btn" data-toggle="modal">Launch Modal</a>
    </h1>

    <div class="modal hide fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
            <h3 id="myModalLabel">Sunday, January 15 2012</h3>
        </div>
        <div class="modal-body">
            <div class="two_col_content_wrap">
                <div class="two_col_content">
                    <h2>Event Name</h2>
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#">Event Overview</a>
                        </li>
                        <li><a href="#">Invite</a></li>
                        <li><a href="#">Privacy</a></li>
                    </ul>
                </div>
            </div>
            <div class="two_col_left">
                <form>
                    <legend>Add Event</legend>
                    <div class="input-append">
                        <input class="span2" id="appendedInputButton" size="16" type="text" placeholder="Event Name"><button class="btn" type="button">Add</button>
                    </div>
                </form>
                <div class="btn-group btn-group-vertical">

                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hideen="true">Close</button>
            <button class="btn btn-primary">Save changes</button>
        </div>
    </div>
</div>
<script type="text/html" id='template_week'>
    <div class="week">
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d0 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d1 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d2 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d3 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d4 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d5 %></span>
            </div>
        </div>
        <div class="day_wrap">
            <div class="day">
                <span class="day_num"><%= d6 %></span>
            </div>
        </div>
    </div>
</script>
</body>
</html>
